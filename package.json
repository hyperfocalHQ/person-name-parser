{
  "name": "person-name-parser",
  "description": "Utility that attempts to parse a person's name into its constituent parts, including a confidence rating of the output",
  "author": "Madi Ostoja",
  "version": "1.0.4",
  "type": "module",
  "main": "./dist/parser.js",
  "module": "./dist/parser.js",
  "types": "./dist/parser.d.ts",
  "exports": {
    ".": {
      "types": "./dist/parser.d.ts",
      "import": "./dist/parser.js",
      "default": "./dist/parser.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "scripts": {
    "test": "vitest run --coverage",
    "build": "tsdown",
    "check": "tsc",
    "prepublishOnly": "npm run build"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperfocalHQ/person-name-parser.git"
  },
  "bugs": {
    "url": "https://github.com/hyperfocalHQ/person-name-parser/issues"
  },
  "homepage": "https://github.com/hyperfocalHQ/person-name-parser",
  "devDependencies": {
    "@types/node": "^24.6.2",
    "@vitest/coverage-v8": "^3.2.4",
    "tsdown": "0.17.0-beta.5",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
